                    const usedKeys = new Map()
                    
                    Object.entries(keybindings).forEach(([action, keys]) => {
                      if (usedKeys.has(keys)) {
                        conflicts.push({ keys, actions: [usedKeys.get(keys), action] })
                      } else {
                        usedKeys.set(keys, action)
                      }
                    })
                    
                    return conflicts.length > 0 ? (
                      conflicts.map(({ keys, actions }, i) => (
                        <div key={i} className="p-2 bg-red-900/20 border border-red-800 rounded">
                          <div className="flex items-center gap-2 text-xs text-red-400">
                            <i className="ph ph-warning"></i>
